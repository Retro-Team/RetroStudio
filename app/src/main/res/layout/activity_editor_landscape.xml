<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:drawer="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:background="#0099cc"
    tools:context="com.retroteam.studio.retrostudio.EditorLandscape"
    android:id="@+id/track_base">

    <TextView android:id="@+id/fullscreen_content" android:layout_width="match_parent"
        android:layout_height="match_parent" android:keepScreenOn="true" android:textColor="#33b5e5"
        android:textStyle="bold" android:textSize="50sp" android:gravity="center"
        android:alpha="50"
        android:text="@string/track_text" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/track_view" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/track_layout0">

            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/track_container0"
                android:background="#79000000"
                android:fillViewport="false"
                android:clickable="true"
                android:outlineProvider="bounds">

                <GridLayout
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:rowCount="1"
                    android:columnCount="100"
                    android:id="@+id/track0"
                    android:background="#79000000">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="1"
                        android:id="@+id/textView2"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="0"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="match_parent"
                        android:text="1a"
                        android:id="@+id/textView5"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="1"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center"
                        android:clickable="true"
                        android:background="@drawable/measure_outline"
                        android:contextClickable="false"
                        android:onClick="editMeasure" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="match_parent"
                        android:text="1b"
                        android:id="@+id/textView6"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="2"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center"
                        android:clickable="true"
                        android:background="@drawable/measure_outline"
                        android:contextClickable="false"
                        android:onClick="editMeasure" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="match_parent"
                        android:text="1c"
                        android:id="@+id/textView8"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="3"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center"
                        android:clickable="true"
                        android:background="@drawable/measure_outline"
                        android:contextClickable="false"
                        android:onClick="editMeasure" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="match_parent"
                        android:text="1d"
                        android:id="@+id/textView9"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="4"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center"
                        android:clickable="true"
                        android:background="@drawable/measure_outline"
                        android:contextClickable="false"
                        android:onClick="editMeasure" />

                </GridLayout>
            </HorizontalScrollView>

            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/track_container1"
                android:background="#79000000"
                android:fillViewport="false"
                android:clickable="true"
                android:outlineProvider="bounds">

                <GridLayout
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:rowCount="1"
                    android:columnCount="100"
                    android:id="@+id/track1"
                    android:background="#79000000">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="2"
                        android:id="@+id/textView"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="0"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="match_parent"
                        android:text="2a"
                        android:id="@+id/textView7"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="1"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center"
                        android:clickable="true"
                        android:background="@drawable/measure_outline"
                        android:contextClickable="false"
                        android:onClick="editMeasure" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="match_parent"
                        android:text="2b"
                        android:id="@+id/textView10"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="2"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center"
                        android:clickable="true"
                        android:background="@drawable/measure_outline"
                        android:contextClickable="false"
                        android:onClick="editMeasure" />
                </GridLayout>
            </HorizontalScrollView>

            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/track_container2"
                android:background="#79000000"
                android:fillViewport="false"
                android:longClickable="true"
                android:clickable="true">

                <GridLayout
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:rowCount="1"
                    android:columnCount="100"
                    android:id="@+id/track2"
                    android:background="#79000000" >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="3"
                        android:id="@+id/textView3"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="0"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="match_parent"
                        android:text="3a"
                        android:id="@+id/textView11"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="1"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center"
                        android:clickable="true"
                        android:background="@drawable/measure_outline"
                        android:contextClickable="false"
                        android:onClick="editMeasure" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="match_parent"
                        android:text="3b"
                        android:id="@+id/textView14"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="2"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center"
                        android:clickable="true"
                        android:background="@drawable/measure_outline"
                        android:contextClickable="false"
                        android:onClick="editMeasure" />
                </GridLayout>
            </HorizontalScrollView>

            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/track_container3"
                android:background="#79000000"
                android:fillViewport="false"
                android:longClickable="true"
                android:clickable="true"
                android:focusable="false">

                <GridLayout
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:rowCount="1"
                    android:columnCount="100"
                    android:id="@+id/track3"
                    android:background="#79000000" >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="4"
                        android:id="@+id/textView4"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="0"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="match_parent"
                        android:text="4a"
                        android:id="@+id/textView12"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="2"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center"
                        android:clickable="true"
                        android:background="@drawable/measure_outline"
                        android:contextClickable="false"
                        android:onClick="editMeasure" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="match_parent"
                        android:text="4b"
                        android:id="@+id/textView13"
                        android:textAlignment="gravity"
                        android:textSize="50dp"
                        android:layout_row="0"
                        android:layout_column="3"
                        android:autoText="false"
                        android:phoneNumber="false"
                        android:gravity="center"
                        android:clickable="true"
                        android:background="@drawable/measure_outline"
                        android:contextClickable="false"
                        android:onClick="editMeasure" />
                </GridLayout>
            </HorizontalScrollView>
        </LinearLayout>
    </ScrollView>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_gravity="bottom"
        android:id="@+id/notedrawercontainer">
    </FrameLayout>


</FrameLayout>
